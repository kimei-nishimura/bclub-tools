{
  "name": "bclub-tools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:popup && npm run build:background && npm run build:content-script && npm run build:injectable-script",
    "build:debug": "npm run build:popup:debug && npm run build:background:debug && npm run build:content-script:debug && npm run build:injectable-script:debug",
    "build2:debug": "npm run build:background:debug && npm run build:content-script:debug && npm run build:injectable-script:debug",
    "build:popup": "ng build --aot --prod --output-hashing=none --source-map=false",
    "build:popup:debug": "ng build --aot",
    "build:background": "webpack-cli --config ./projects/background/webpack.config.js --context ./projects/background/ --mode=production",
    "build:background:debug": "webpack-cli --config ./projects/background/webpack.config.js --context ./projects/background/ --mode=development",
    "build:content-script": "webpack-cli --config ./projects/content-script/webpack.config.js --context ./projects/content-script/ --mode=production",
    "build:content-script:debug": "webpack-cli --config ./projects/content-script/webpack.config.js --context ./projects/content-script/ --mode=development",
    "build:injectable-script": "webpack-cli --config ./projects/injectable-script/webpack.config.js --context ./projects/injectable-script/ --mode=production",
    "build:injectable-script:debug": "webpack-cli --config ./projects/injectable-script/webpack.config.js --context ./projects/injectable-script/ --mode=development",
    "tool:update-texts": "node ./tools/update-texts.js 8564c09289bb2dfb2cac52b5e4480bde05926a6f",
    "package": "node ./release/create-package.js",
    "test": "ng test",
    "test:ci": "npm run test -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.3",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "~8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "~8.1.0",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "~8.1.0",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/router": "~8.1.0",
    "jszip": "^3.7.0",
    "lodash-es": "^4.17.11",
    "lz-string": "^1.4.4",
    "papaparse": "^5.2.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "~8.1.0",
    "@angular/language-service": "~8.1.0",
    "@types/chrome": "0.0.86",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "~8.9.4",
    "@types/sinon-chrome": "^2.2.10",
    "archiver": "^3.1.1",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-mocks": "^11.9.0",
    "protractor": "~5.4.0",
    "sinon-chrome": "^3.0.1",
    "socket.io-client": "^4.2.0",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.5",
    "webpack-cli": "^3.3.5"
  }
}
